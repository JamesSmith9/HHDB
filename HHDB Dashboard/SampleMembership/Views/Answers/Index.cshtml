@model IEnumerable<SampleMembership.Models.Answer>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Answer
        </th>
        <th>
            Created By User:
        </th>
        <th>
            @Html.ActionLink("Month", "Index", new { sortOrder = ViewBag.DateSortParm })
        </th>
        <th>
           @Html.ActionLink("Year", "Index", new { sortOrder = ViewBag.NameSortParm})
        </th>
        <th>
          Quantity
        </th>
        <th>
            Question ID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AnsText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedByUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Month)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SurveyXQuestion.SXQID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AnswerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AnswerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AnswerID })
        </td>
    </tr>
}

</table>
